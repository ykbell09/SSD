  },
  "homepage": "https://github.com/ykbell09/SSD#readme",
  "dependencies": {
    "bcrypt": "^4.0.1",
    "connect-session-knex": "^1.6.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "express-session": "^1.17.0",
    "graphql": "^14.6.0",
    "knex": "^0.20.12",
    "pg": "^7.18.2"
  }
}

{
  "name": "ssd_proj",
  "version": "1.0.0",
  "description": "last two weeks assignments",
  "main": "app.js",
  "scripts": {
    "make-db": "dropdb --username=postgres --if-exists current && createdb --username=postgres current && psql -U postgres -d current -f sql/schema.sql && psql -U postgres -d current -f sql/seed.sql",
    "build": "npm run migrate; npm run seed",
    "migrate": "npm run knex -- migrate:latest",
    "seed": "npm run knex -- seed:run",
    "start": "node -r esm app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ykbell09/SSD.git"
  },
